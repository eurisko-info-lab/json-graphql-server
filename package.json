{
    "name": "json-graphql-server",
    "version": "3.1.2",
    "type": "module",
    "main": "./dist/json-graphql-server.cjs",
    "module": "./dist/json-graphql-server.js",
    "types": "./dist/json-graphql-server.d.ts",
    "exports": {
        ".": {
            "import": "./dist/json-graphql-server.js",
            "require": "./dist/json-graphql-server.cjs"
        },
        "./node": {
            "import": "./dist/json-graphql-server-node.js",
            "require": "./dist/json-graphql-server-node.cjs"
        }
    },
    "repository": "git@github.com:marmelab/json-graphql-server.git",
    "authors": [
        "Fran√ßois Zaninotto",
        "Gildas Garcia"
    ],
    "files": [
        "*.md",
        "dist",
        "src",
        "bin"
    ],
    "license": "MIT",
    "scripts": {
        "format": "make format",
        "precommit": "lint-staged",
        "test": "jest",
        "watch-test": "make watch-test",
        "server": "make run",
        "prepublish": "make build",
        "build": "tsc",
        "watch": "tsc --watch"
    },
    "lint-staged": {
        "src/**/*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/graphql-fields": "^1.3.9",
        "@types/jest": "^29.5.14",
        "@types/lodash.merge": "^4.6.7",
        "@types/node": "^22.10.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "@typescript-eslint/parser": "^6.5.0",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^29.7.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^28.2.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "supertest": "^6.3.4",
        "vite": "^5.2.14",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@apollo/client": "^3.9.11",
        "@graphql-tools/schema": "^10.0.3",
        "cors": "^2.8.5",
        "express": "^4.20.0",
        "graphql": "^16.8.1",
        "graphql-http": "^1.22.1",
        "graphql-tag": "^2.12.6",
        "graphql-type-json": "^0.3.2",
        "inflection": "^3.0.0",
        "lodash.merge": "^4.6.2",
        "reify": "^0.20.12",
        "xhr-mock": "^2.5.1"
    },
    "bin": {
        "json-graphql-server": "bin/json-graphql-server.cjs"
    },
    "compilerOptions": {
        "typeRoots": [
            "node_modules/@types"
        ]
    }
}
